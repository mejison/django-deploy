{"ast":null,"code":"import axios from 'axios';\nimport ProductBox from '@/components/ProductBox';\nexport default {\n  name: 'Home',\n  data() {\n    return {\n      latestProducts: []\n    };\n  },\n  components: {\n    ProductBox\n  },\n  mounted() {\n    this.getLatestProducts();\n  },\n  methods: {\n    async getLatestProducts() {\n      this.$store.commit('setIsLoading', true);\n      await axios.get('/api/v1/latest-products/').then(response => {\n        this.latestProducts = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n      this.$store.commit('setIsLoading', false);\n    }\n  }\n};","map":{"version":3,"mappings":"AAgCA,OAAOA,KAAI,MAAO,OAAM;AAExB,OAAOC,UAAS,MAAO,yBAAwB;AAE/C,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,IAAI,GAAG;IACL,OAAO;MACLC,cAAc,EAAE;IAClB;EACF,CAAC;EACDC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,OAAO,GAAG;IACR,IAAI,CAACC,iBAAiB,EAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,iBAAiB,GAAG;MACxB,IAAI,CAACE,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,IAAI;MAEvC,MAAMV,KAAI,CACNW,GAAG,CAAC,0BAA0B,EAC9BC,IAAI,CAACC,QAAO,IAAK;QACf,IAAI,CAACT,cAAa,GAAIS,QAAQ,CAACV,IAAG;MACrC,CAAC,EACAW,KAAK,CAACC,KAAI,IAAK;QACbC,OAAO,CAACC,GAAG,CAACF,KAAK;MACpB,CAAC;MAEJ,IAAI,CAACN,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,KAAK;IAC1C;EACF;AACF","names":["axios","ProductBox","name","data","latestProducts","components","mounted","getLatestProducts","methods","$store","commit","get","then","response","catch","error","console","log"],"sourceRoot":"","sources":["C:\\Users\\shahe\\Desktop\\VS project\\django_vue\\src\\views\\Home.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n     <section class=\"hero is-medium is-dark mb-6\">\n        <div class=\"hero-body has-text-centered\">\n            <p class=\"title mb-6\">\n                Welcome to Djacket\n            </p>\n            <p class=\"subtitle\">\n                The best jacket store online\n            </p>\n        </div>\n    </section>\n\n    <div class=\"columns is-multiline\">\n      <div class=\"column is-12\">\n          <h2 class=\"is-size-2 has-text-centered\">Latest products</h2>\n      </div>\n\n      <ProductBox\n        v-for=\"product in latestProducts\"\n        v-bind:key=\"product.id\"\n        v-bind:product=\"product\" />\n      \n\n       \n\n     </div>\n  </div>\n  \n</template>\n\n<script>\nimport axios from 'axios'\n\nimport ProductBox from '@/components/ProductBox'\n\nexport default {\n  name: 'Home',\n  data() {\n    return {\n      latestProducts: []\n    }\n  },\n  components: {\n    ProductBox\n  },\n  mounted() {\n    this.getLatestProducts()\n  },\n  methods: {\n    async getLatestProducts() {\n      this.$store.commit('setIsLoading', true)\n\n      await axios\n         .get('/api/v1/latest-products/')\n         .then(response => {\n            this.latestProducts = response.data\n         })\n         .catch(error => {\n            console.log(error)\n         })\n        \n      this.$store.commit('setIsLoading', false)\n    }\n  }\n}\n\n</script>\n\n<style scoped>\n  .image {\n    margin-top: -1.25rem;\n    margin-left: -1.25rem;\n    margin-right: -1.25rem;\n  }\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}